nodeKeyword         =  %x6E %x6F %x64 %x65
representsKeyword   =  %x72 %x65 %x70 %x72 %x65 %x73 %x65 %x6E %x74
                          %x73
scalarsKeyword      =  %x73 %x63 %x61 %x6C %x61 %x72 %x73
tableKeyword        =  %x74 %x61 %x62 %x6C %x65

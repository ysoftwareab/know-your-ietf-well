Route  =  "Route" ":" 1# name-addr


Subject  =  ( "Subject" | "s" ) ":" *TEXT-UTF8


Timestamp  =  "Timestamp" ":" *(DIGIT) [ "." *(DIGIT) ] [ delay ]
delay      =  *(DIGIT) [ "." *(DIGIT) ]


To  =  ( "To" | "t" ) ":" ( name-addr | addr-spec )

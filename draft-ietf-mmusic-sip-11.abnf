                             1#content-coding


Content-Length  =  ( "Content-Length" | "l" ) ":" 1*DIGIT


Content-Type  =  ( "Content-Type" | "c" ) ":" media-type


CSeq  =  "CSeq" ":" 1*DIGIT Method



SIP-date  =  rfc1123-date



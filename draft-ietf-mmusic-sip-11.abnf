addr-params  =  tag-param
tag-param    =  "tag=" UUID
UUID         =  1*( hex | "-" )


Hide  =  "Hide" ":" ( "route" | "hop" )


Max-Forwards  =  "Max-Forwards" ":" 1*DIGIT


Organization  =  "Organization" ":" *TEXT-UTF8


Priority        =  "Priority" ":" priority-value
